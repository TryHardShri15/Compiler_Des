LEX FILE 
%{ 
#include "y.tab.h" 
%} 
%% 
[0-9]+      { yylval = atoi(yytext); return NUMBER; } 
[\+\-\*\/]  { return yytext[0]; } 
[\(\)]      { return yytext[0]; } 
[ \t\n]     ; 
.           ; 
%% 
 
 
YACC FILE 
%{ 
#include <stdio.h> 
#include <stdlib.h> 
 
int yylex(); 
int yyerror(char *s); 
%} 
 
%token NUMBER 
 
%% 
S : E { printf("Result = %d\n", $1); } 
  ; 
 
E : E '+' T   { $$ = $1 + $3; } 
  | E '-' T   { $$ = $1 - $3; } 
  | T 
  ; 
 
T : T '*' F   { $$ = $1 * $3; } 
  | T '/' F   { $$ = $1 / $3; } 
  | F 
  ; 
 
F : NUMBER 
  | '(' E ')' { $$ = $2; } 
  ; 
%% 
 
int main() { 
    printf("Enter Expression: "); 
    yyparse(); 
    return 0; 
} 
 
int yyerror(char *s) { 
    printf("Invalid Expression\n"); 
    return 0; 
}

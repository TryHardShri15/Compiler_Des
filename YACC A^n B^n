LEX FILE 
 
%{ 
#include "y.tab.h" 
%} 
 
%% 
 
a      { return A; } 
b      { return B; } 
\n     { return 0; } 
.      { return INVALID; } 
 
%% 
YACC FILE 
 
%{ 
#include <stdio.h> 
#include <stdlib.h> 
 
int yylex(); 
int yyerror(char *s); 
int valid = 1; 
%} 
 
%token A B INVALID 
 
%% 
S : A S B 
  | A B 
  ; 
%% 
 
int main() { 
    printf("Enter string: "); 
    yyparse(); 
     
    if(valid) 
        printf("Valid String (a^n b^n)\n"); 
    else 
        printf("Invalid String\n"); 
 
    return 0; 
} 
 
int yyerror(char *s) { 
    valid = 0; 
    return 0; 
}

Example : aabb

OUTPUT:
Valid String (a^n b^n)
